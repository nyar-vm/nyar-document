<h2 id="modules" class="small-section-header">
    <a href="#modules">Modules</a>
</h2>


<ul class="item-table"></ul>
{% for module in modules %}
<li>
    <div class="item-name">
        <a class="type-module" href="filters/index.html" title="modio::comments::filters">{{ module.name }}</a>
    </div>
    <div class="desc docblock-short">{{ module.summary }}</div>
</li>
{% endfor %}
</ul>


<h2 id="interfaces" class="small-section-header">
    <a href="#interfaces">Interfaces</a>
</h2>
<ul class="item-table"></ul>
{% for interface in interfaces %}
<li>
    <div class="item-name">
        <a class="type-interface" href="filters/index.html" title="modio::comments::filters">
            {{ interface.get_name() }}
        </a>
    </div>
    <div class="desc docblock-short">{{ interface.get_summary() }}</div>
</li>
{% endfor %}
</ul>

<h2 id="structures" class="small-section-header">
    <a href="#structures">Structures</a>
</h2>
<ul class="item-table"></ul>
{% for structure in structures %}
<li>
    <div class="item-name">
        <a class="type-structure" href="filters/index.html" title="modio::comments::filters">
            {{ structure.get_name() }}
        </a>
    </div>
    <div class="desc docblock-short">{{ structure.get_summary() }}</div>
</li>
{% endfor %}
</ul>

<style>
    .type {
        color: #E2931D;
    }

    .type-module {
        color: #4078F2;
    }

    .type-structure {
        color: #E2931D;
    }

    .type-interface {
        color: #50A14E;

    }
</style>